<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Link" table="link">
    <id name="expediteurId" type="integer" column="expediteur_id"/>
    <id name="destinataireId" type="integer" column="destinataire_id"/>
    <field name="status" type="integer" column="status" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="modificationDate" type="date" column="modification_date" nullable="false"/>
  </entity>
</doctrine-mapping>
