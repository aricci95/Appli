<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Article" table="article">
    <id name="artId" type="integer" column="art_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="artLibel" type="string" column="art_libel" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="categorieId" type="integer" column="categorie_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">5</option>
      </options>
    </field>
    <field name="artDescription" type="text" column="art_description" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="refStateId" type="integer" column="ref_state_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="artPrice" type="integer" column="art_price" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="artDate" type="datetime" column="art_date" nullable="false"/>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="artPhotoUrl" type="string" column="art_photo_url" length="80" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="artColour" type="string" column="art_colour" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="artPoids" type="integer" column="art_poids" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="artTaille" type="integer" column="art_taille" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="artMarque" type="string" column="art_marque" length="35" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="livrePoste" type="integer" column="livre_poste" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="livreSurplace" type="integer" column="livre_surplace" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="artStateId" type="integer" column="art_state_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
