<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Covoit" table="covoit">
    <id name="covoitId" type="integer" column="covoit_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="concertId" type="integer" column="concert_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateDepart" type="datetime" column="date_depart" nullable="false"/>
    <field name="dateRetour" type="datetime" column="date_retour" nullable="false"/>
    <field name="price" type="integer" column="price" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="villeId" type="integer" column="ville_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
