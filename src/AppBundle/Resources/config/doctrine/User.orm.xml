<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\User" table="user" repository-class="AppBundle\Entity\Repository\UserRepository">
    <id name="userId" type="integer" column="user_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userLogin" type="string" column="user_login" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userPwd" type="string" column="user_pwd" length="35" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userGender" type="integer" column="user_gender" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userSubscribeDate" type="datetime" column="user_subscribe_date" nullable="false"/>
    <field name="roleId" type="integer" column="role_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userLastConnexion" type="datetime" column="user_last_connexion" nullable="false"/>
    <field name="userBirth" type="date" column="user_birth" nullable="false"/>
    <field name="eyesId" type="integer" column="eyes_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="originId" type="integer" column="origin_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userDescription" type="text" column="user_description" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userLightDescription" type="string" column="user_light_description" length="77" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userCity" type="text" column="user_city" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userZipcode" type="string" column="user_zipcode" length="5" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userPhotoUrl" type="text" column="user_photo_url" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userAlcohol" type="integer" column="user_alcohol" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userSmoke" type="integer" column="user_smoke" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userTattoo" type="integer" column="user_tattoo" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userDrugs" type="integer" column="user_drugs" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="styleId" type="integer" column="style_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hairId" type="integer" column="hair_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lookId" type="integer" column="look_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userPiercing" type="integer" column="user_piercing" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userProfession" type="string" column="user_profession" length="35" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userMusic" type="text" column="user_music" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userPoids" type="integer" column="user_poids" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userTaille" type="integer" column="user_taille" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="weekUser" type="integer" column="week_user" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="userMail" type="string" column="user_mail" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userValid" type="string" column="user_valid" length="35" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="listId" type="integer" column="list_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="villeId" type="integer" column="ville_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
