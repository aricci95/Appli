<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TmpMail" table="tmp_mail">
    <id name="mailId" type="integer" column="mail_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mailContent" type="text" column="mail_content" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mailExpediteur" type="integer" column="mail_expediteur" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mailDestinataire" type="integer" column="mail_destinataire" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mailDate" type="datetime" column="mail_date" nullable="false"/>
    <field name="mailStateId" type="integer" column="mail_state_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mailboxId" type="integer" column="mailbox_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
