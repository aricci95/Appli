<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\News" table="news">
    <id name="newsId" type="integer" column="news_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="newsTitre" type="string" column="news_titre" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="newsContenu" type="text" column="news_contenu" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="newsDate" type="datetime" column="news_date" nullable="false"/>
    <field name="newsTypeId" type="integer" column="news_type_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="newsMediaUrl" type="text" column="news_media_url" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="newsPhotoUrl" type="text" column="news_photo_url" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
